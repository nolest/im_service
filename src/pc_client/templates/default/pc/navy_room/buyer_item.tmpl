{{#each data}}
<div class="buyer_list_item ds-box orient-h align-center" data-role="buyer_list_item" manage_id="{{manage_id}}" custom_id="{{custom_id}}" send_user_id="{{send_user_id}}" to_user_id="{{to_user_id}}" data-buyer-list-item="{{manage_id}}{{custom_id}}" data-async-buyer="{{custom_id}}">
    <img class="ds-block" src='{{custom_icon}}' style="width: 40px;height: 40px;border-radius: 2px" data-async="icon" data-role="face-to-seller-info"/>
    <div class="ds-box orient-v nickname">
        <div data-async="name">{{custom_name}}</div>
        <div>ID:{{custom_id}}</div>
    </div>
    <div class="point none" data-role="nums" data-nums="0">0</div>
</div>
{{/each}}