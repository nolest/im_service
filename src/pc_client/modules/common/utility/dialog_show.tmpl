<div class="ui-dialog" data-role="ui-dialog">
    <div class="ui-dialog-cnt">
        <div class="ui-dialog-bd">
            <div>
                {{#if title}}
                    <h4>{{title}}</h4>
                {{/if}}  
                <div>{{content}}</div>
            </div>
        </div>
        <div class="ui-dialog-ft ui-btn-group">
            <button type="button" data-role="ui-dialog-button"  class="select" >关闭</button> 
        </div>
    </div>
</div>